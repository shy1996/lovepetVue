<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/lib/bootstrap/dist/css/bootstrap.min.css' />
    <link rel='stylesheet' href='/stylesheets/font-awesome.min.css' />
    <link rel='stylesheet' href='/stylesheets/ionicons.min.css' />
    <link rel='stylesheet' href='/lib/admin-lte/dist/css/AdminLTE.min.css' />
    <link rel='stylesheet' href='/lib/admin-lte/dist/css/skins/skin-blue.min.css' />
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body class="hold-transition skin-blue sidebar-mini">
  <div class="wrapper">

    <% include header.ejs %>

    <% include menu.ejs %>
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Main content -->
      <section class="content">

        <!-- Horizontal Form -->
        <div class="box box-info">
          <div class="box-header with-border">
            <h3 class="box-title">添加商品信息</h3>
          </div>
          <!-- /.box-header -->
          <!-- form start -->
          <form class="form-horizontal" action="/list/addProductAction?pageCode=<%= pageCode %>" method="post">
            <div class="box-body">
              <div class="form-group">
                <label for="item_id" class="col-sm-2 control-label">商品ID：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control"  name="item_id" placeholder="商品ID" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="title" class="col-sm-2 control-label">商品名称：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="title" placeholder="商品名称" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="url" class="col-sm-2 control-label">商品URL链接：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="url" placeholder="商品URL链接" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="priceWap" class="col-sm-2 control-label">价格：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="priceWap" placeholder="价格Wap " value="">
                </div>
              </div>
              <div class="form-group">
                <label for="type" class="col-sm-2 control-label">商品类型：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="type" placeholder="商品类型" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="tItemType" class="col-sm-2 control-label">tItemType：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control"  name="tItemType" placeholder=" tItemType" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="isP4p" class="col-sm-2 control-label">是否是P4p：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="isP4p" placeholder="是否是P4p" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="sold" class="col-sm-2 control-label">销量：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control"  name="sold" placeholder="销量" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="shipping" class="col-sm-2 control-label">运送费：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="shipping" placeholder="运送费" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="fastPostFee" class="col-sm-2 control-label">邮递费：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="fastPostFee" placeholder="邮递费" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="location" class="col-sm-2 control-label">地址：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="location" placeholder="地址" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="userId" class="col-sm-2 control-label">用户ID：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control"  name="userId" placeholder="用户ID" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="userType" class="col-sm-2 control-label">用户类型：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control"  name="userType" placeholder="用户类型" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="nick" class="col-sm-2 control-label">店名：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control"  name="nick" placeholder="店名" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="area" class="col-sm-2 control-label">地区：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control"  name="area" placeholder="地区" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="pic_path" class="col-sm-2 control-label">图片地址：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="pic_path" placeholder="图片地址" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="priceColor" class="col-sm-2 control-label">价格颜色：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="priceColor" placeholder="价格颜色" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="zkType" class="col-sm-2 control-label">手机专享价：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="zkType" placeholder="手机专享价" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="itemNumId" class="col-sm-2 control-label">手机产品ID：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="itemNumId" placeholder="手机产品ID"  value="">
                </div>
              </div>
              <div class="form-group">
                <label for="name" class="col-sm-2 control-label">手机产品名称：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="name" placeholder="手机产品名称" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="img2" class="col-sm-2 control-label">手机商品图片：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="img2" placeholder="手机商品图片" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="wwimUrl" class="col-sm-2 control-label">手机wwimUrl：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="wwimUrl" placeholder="手机wwimUrl" value="">
                </div>
              </div>
              <div class="form-group">
                <label for="previewUrl" class="col-sm-2 control-label">previewUrl：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="previewUrl" placeholder="previewUrl"  value="">
                </div>
              </div>
              <div class="form-group">
                <label for="favoriteUrl" class="col-sm-2 control-label">favoriteUrl：</label>

                <div class="col-sm-8">
                  <input type="text" class="form-control" name="favoriteUrl" placeholder="favoriteUrl" value="">
                </div>
              </div>

            </div>
            <div class="form-group">
              <label for="price" class="col-sm-2 control-label">手机商品价格：</label>

              <div class="col-sm-8">
                <input type="text" class="form-control" name="price" placeholder="手机商品价格" value="">
              </div>
            </div>
            <div class="form-group">
              <label for="originalPrice" class="col-sm-2 control-label">originalPrice：</label>

              <div class="col-sm-8">
                <input type="text" class="form-control" name="originalPrice" placeholder="originalPrice" value="">
              </div>
            </div>
            <div class="form-group">
              <label for="isMobileEcard" class="col-sm-2 control-label">isMobileEcard：</label>

              <div class="col-sm-8">
                <input type="text" class="form-control" name="isMobileEcard" placeholder="isMobileEcard" value="">
              </div>
            </div>
            <div class="form-group">
              <label for="freight" class="col-sm-2 control-label">freight：</label>

              <div class="col-sm-8">
                <input type="text" class="form-control" name="freight" placeholder="freight" value="">
              </div>
            </div>
            <div class="form-group">
              <label for="act" class="col-sm-2 control-label">act：</label>

              <div class="col-sm-8">
                <input type="text" class="form-control" name="act" placeholder="act" value="">
              </div>
            </div>
            <div class="form-group">
              <label for="coinLimit" class="col-sm-2 control-label">coinLimit：</label>

              <div class="col-sm-8">
                <input type="text" class="form-control" name="coinLimit" placeholder="coinLimit" value="">
              </div>
            </div>


            <div class="form-group">
              <label for="editor1" class="col-sm-2 control-label">电影简介：</label>

              <div class="col-sm-8">
                  <textarea id="editor1" name="editor1" rows="10" class="form-control" placeholder="请输入电影简介···" style="resize: none;">

                    </textarea>
              </div>
            </div>



            <!-- /.box-body -->
            <div class="box-footer">
              <button type="button" class="btn btn-default" onclick="backNews(<%= pageCode %>)">
                返回
              </button>
              <button type="submit" class="btn btn-info pull-right" id="savaProductBtn">
                保存
              </button>
            </div>
            <!-- /.box-footer -->
          </form>
        </div>


      </section>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->

    <!-- Main Footer -->
    <% include footer.ejs %>


    <!-- Add the sidebar's background. This div must be placed
         immediately after the control sidebar -->
    <div class="control-sidebar-bg"></div>
  </div>
  <!-- ./wrapper -->
  
  
  </body>
  <script src = "/javascripts/jquery.min.js"></script>
  <script src = "/lib/bootstrap/dist/js/bootstrap.min.js"></script>
  <script src = "/lib/ckeditor/ckeditor.js"></script>
  <script src = "/javascripts/app.min.js"></script>
  <script>
      $(function () {
          // Replace the <textarea id="editor1"> with a CKEditor
          // instance, using default configuration.
          CKEDITOR.replace('editor1');
          //bootstrap WYSIHTML5 - text editor
          $(".textarea").wysihtml5();
      });
      function backNews(pageCode) {
          window.location.href="/list/backList?pageCode="+pageCode;
      }
  </script>

</html>
